        </trim>
    </insert>

    <!--auto generated Code-->
    <insert id="insertList">
        INSERT INTO shopping_cart(
        <include refid="all_column"/>
        )VALUES
        <foreach collection="pojos" item="pojo" index="index" separator=",">
            (
            #{pojo.id},
